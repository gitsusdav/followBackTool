@using HtmlAgilityPack
@using System.Globalization;
@{
    var lang = ViewData["Language"].ToString() ?? "es"; 
    string title = "";
    if (lang == "es")
    { title = "Resultados";
    }
    else if (lang == "en") 
    { 
        title = "Results"; 
    } 
    else 
    {
        title = "Resultados";
    }
    ViewData["Title"] = title;
    @model User;
}
<h1 class="text-white" id="results-h1">@ViewData["Title"]</h1>

<p class="text-white" id="lead-results">Los usuarios que no te siguen de vuelta son:</p>

<div class="overflow-auto p-3 mb-3 bg-light" style="max-height: 30em;">
    @foreach (var item in Model.notMutuals)
    {
        <p><a href="https://www.instagram.com/@item/" target="_blank">@item</a></p>
    }
</div>
